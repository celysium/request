{
    "name": "celysium/request",
    "description": "A simple package to send request.",
    "autoload": {
        "psr-4": {
            "Celysium\\Request\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nasser Hekmati",
            "email": "hekmati.nasser@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework" : "^9.0|^10.0",
        "celysium/responser": "^1.1",
        "celysium/authenticate": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
              "Celysium\\Request\\RequestBuilderServiceProvider"
            ],
            "aliases": {
                "RequestBuilder": "Celysium\\Request\\Facades\\RequestBuilder"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
